<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PriceFinding.Styles"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">
     <System:Double x:Key="dblCornerRadius">2.35</System:Double>
     <System:Double x:Key="dblCornerRadius_TL">2.35</System:Double>
     <System:Double x:Key="dblCornerRadius_TR">2.35</System:Double>
     <System:Double x:Key="dblCornerRadius_BL">2.35</System:Double>
     <System:Double x:Key="dblCornerRadius_BR">2.35</System:Double>


     <CornerRadius x:Key="CornerRadiusPrimary" 
                   TopLeft="{StaticResource dblCornerRadius}" 
                   TopRight="{StaticResource dblCornerRadius}"
                   BottomLeft="{StaticResource dblCornerRadius}" 
                   BottomRight="{StaticResource dblCornerRadius}" >
          
     </CornerRadius>
</ResourceDictionary>